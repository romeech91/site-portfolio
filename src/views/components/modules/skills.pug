script(type='text/x-template')#skills-list
    ul.skill-list
        li.skill-list__item.skills-row(v-for="skill in skills")
            skills-row(
                :skillGroupName="skill.skillsGroup"
            )

script(type="text/x-template")#skills-row
    .skills-row__title {{ skillGroupName }}
        ul.skill-row__list
            li.skill-row__item
                
script(type="text/x-template")#skill
    figure.skill
        svg(viewbox='0 0 100 100' preserveAspectRatio='none').skill__figure
            circle(cx='50' cy='50' r='40').skill__circle.skill__circle_inder
            circle(cx='50' cy='50' r='40').skill__circle.skill__circle_above
        .skill__title CSS3